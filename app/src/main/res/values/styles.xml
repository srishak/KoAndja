<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppThemeand" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryand</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkand</item>
        <item name="colorAccent">@color/colorAccentand</item>
    </style>

    <style name="AppThemekot" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimarykot</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkkot</item>
        <item name="colorAccent">@color/colorAccentkot</item>
    </style>

    <style name="AppThemejav" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryjav</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkjav</item>
        <item name="colorAccent">@color/colorAccentjav</item>
    </style>

    <style name="AppThemesc" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimarysc</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarksc</item>
        <item name="colorAccent">@color/colorAccentsc</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>
